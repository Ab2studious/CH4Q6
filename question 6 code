#include <iostream>

using namespace std;

int main()
{
   float weight;
   float mass;
   cout << "Enter mass here" << endl;
    cin>>mass;
    weight = mass * 9.8;
    if (weight > 1000)
    cout << " OMGSH I CAN NOT PICK THIS UP" << endl;
    if (weight < 10)
    cout << "thats way too light its like a feather dude! " << endl;
    weight = mass * 9.8;
    cout<<weight<< "Newtons"<< endl;

   return 0;
}
